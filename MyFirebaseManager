    public void getVideoFileToFirebaseStorage(Uri targetUri, String fileName, final Context context){

        String tURI = targetUri.toString();

        final String extension = tURI.substring(tURI.lastIndexOf("."));
        StorageReference mountainImagesRef = FirebaseStorage.getInstance().getReference(fileName);
        final UploadTask uploadTask = mountainImagesRef.putFile(targetUri);
        uploadTask.addOnFailureListener(new OnFailureListener() {
            @Override
            public void onFailure(@NonNull Exception exception) {
                // Handle unsuccessful uploads
                Toast.makeText(context, "File Failed to Upload", Toast.LENGTH_SHORT).show();
            }
        }).addOnSuccessListener(new OnSuccessListener<UploadTask.TaskSnapshot>() {
            @Override
            public void onSuccess(UploadTask.TaskSnapshot taskSnapshot) {

                StorageMetadata metadata = new StorageMetadata.Builder()
                        .setContentType("video/flv")
                        .build();
                    Toast.makeText(context, "File Successfully Uploaded", Toast.LENGTH_SHORT).show();


            }
        });

    }

    public void getImageFileToFirebaseStorage(Uri targetUri, String fileName, final Context context){

        String tURI = targetUri.toString();


        StorageReference mountainImagesRef = FirebaseStorage.getInstance().getReference(fileName);
        final UploadTask uploadTask = mountainImagesRef.putFile(targetUri);
        uploadTask.addOnFailureListener(new OnFailureListener() {
            @Override
            public void onFailure(@NonNull Exception exception) {
                // Handle unsuccessful uploads
            }
        }).addOnSuccessListener(new OnSuccessListener<UploadTask.TaskSnapshot>() {
            @Override
            public void onSuccess(UploadTask.TaskSnapshot taskSnapshot) {

                StorageMetadata metadata = new StorageMetadata.Builder()
                        .setContentType("image/jpg")
                        .build();

                    Toast.makeText(context, "File Successfully Uploaded", Toast.LENGTH_SHORT).show();


            }
        });

    }

}

