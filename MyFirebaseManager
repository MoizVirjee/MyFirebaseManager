public class MyFirebaseManager {

        public ArrayList<EntryClass> getEntryArray() {
                return entryArray;
        }

        ArrayList<EntryClass> entryArray;
        String tableName;
        String field1Match;
        DatabaseReference firebaseRef;
        int isGetWholeTableToArrayList;

        long dataCount = 0;
        long countYet;
        ArrayList<String> AllfieldsArray;
        MyFirebaseManager myFirebaseManager;
        SQLiteDatabase sqLiteDatabase;

    public EntryClass myEntryClass;

    MyFirebaseManager() { };

    public void setTable(String tableName){
        this.tableName = tableName;
        firebaseRef = FirebaseDatabase.getInstance().getReference(tableName);
    }

    public void postEntry(EntryClass myEntryClass) {


        this.myEntryClass = myEntryClass;
        firebaseRef = FirebaseDatabase.getInstance().getReference(tableName);

        //String mKey = firebaseRef.push().getKey();

        firebaseRef.child(myEntryClass.getField1Data()).setValue(myEntryClass);

    }


