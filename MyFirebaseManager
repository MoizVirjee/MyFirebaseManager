                                        field42String + "," +
                                        field43String + "," +
                                        field44String + "," +
                                        field45String + "," +
                                        field46String + ",";

                        AllfieldsArray.add(allFieldString);*/

                                                countYet = countYet + 1;



                                                String fieldNamesInsertInto = fieldNames[0];


                                                for(int counter=1;counter<fieldNames.length;counter++){

                                                        fieldNamesInsertInto = fieldNamesInsertInto + "," + fieldNames[counter];

                                                }

                                                String fieldDataInsertInto = "'" + fieldStrings[0] + "'";

                                                for(int counter=1;counter<fieldNames.length;counter++){

                                                        fieldDataInsertInto =  fieldDataInsertInto + " ,'" + fieldStrings[counter] + "'";

                                                }

                                                Cursor mCursor = sqLiteDatabase.rawQuery("Select "+fieldNames[0]+" from "+ tableName+" where "+fieldNames[0]+" = '" + fieldStrings[0]+"'",null);

                                                if (mCursor.getCount()>=1){}else {

                                                        sqLiteDatabase.execSQL("INSERT INTO " + tableName + " (" + fieldNamesInsertInto + ") VALUES (" + fieldDataInsertInto + ")");



                                                        Toast.makeText(context, "Sync " + String.valueOf(countYet * 100 / dataCount) + "% Completed", Toast.LENGTH_SHORT).show();

                                                }




                                        }


                                }

                                isGetWholeTableToArrayList=0;
                        }

                        @Override
                        public void onCancelled(DatabaseError databaseError) {
                        }
                });



        }

