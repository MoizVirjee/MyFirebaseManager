                                                fieldStrings[37] = mEntryClass.getField39Data();
                                                fieldStrings[38] = mEntryClass.getField40Data();
                                                fieldStrings[39] = mEntryClass.getField41Data();
                                                fieldStrings[40] = mEntryClass.getField42Data();
                                                fieldStrings[41] = mEntryClass.getField43Data();
                                                fieldStrings[42] = mEntryClass.getField44Data();
                                                fieldStrings[43] = mEntryClass.getField45Data();
                                                fieldStrings[44] = mEntryClass.getField46Data();

