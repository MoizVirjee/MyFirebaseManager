    public void removeTable(){

            firebaseRef = FirebaseDatabase.getInstance().getReference(tableName);
            firebaseRef.removeValue();

    }

    public boolean saveExcelFile(Context context, String fileName, SQLiteDatabase mSqLiteDatabase, String sqlStatement, String[] fieldNames,String sheetName) {


                // check if available and not read only

                boolean success = false;

                //New Workbook
                Workbook wb = new HSSFWorkbook();


                Cell c = null;

                //Cell style for header row
                CellStyle cs = wb.createCellStyle();
                cs.setFillForegroundColor(HSSFColor.LIME.index);
                cs.setFillPattern(HSSFCellStyle.SOLID_FOREGROUND);

                //New Sheet
                Sheet sheet1 = null;
                sheet1 = wb.createSheet(sheetName);

                //run Query in SQLite to retrieve data
                Cursor mCursor = mSqLiteDatabase.rawQuery(sqlStatement,null);
                mCursor.moveToFirst();


                // Generate column headings
                Row currentRow = sheet1.createRow(0);

                File file = new File(context.getExternalFilesDir(DIRECTORY_DOWNLOADS), fileName);
                FileOutputStream os = null;

                currentRow = sheet1.createRow(0);

                for(int counter = 0; counter < fieldNames.length;counter++){

                        c = currentRow.createCell(counter);
                        c.setCellValue(fieldNames[counter]);
                }

                mCursor.moveToFirst();

                for (int counter = 0; counter<mCursor.getCount();counter++){


                        currentRow = sheet1.createRow(counter+1);

                        for (int subCounter = 0; subCounter < fieldNames.length;subCounter++){

                                c = currentRow.createCell(subCounter);
                                String cellDetail = mCursor.getString(mCursor.getColumnIndex(fieldNames[subCounter]));
                                c.setCellValue(cellDetail);


                        }

                        if(counter + 1 < mCursor.getCount()){ mCursor.moveToNext();};
                }

















        /*sheet1.setColumnWidth(0, (15 * 500));
        sheet1.setColumnWidth(1, (15 * 500));
        sheet1.setColumnWidth(2, (15 * 500));*/

                // Create a path where we will place our List of objects on external storage


                try {
                        os = new FileOutputStream(file);
                        wb.write(os);
                        Log.w("FileUtils", "Writing file" + file);
                        success = true;
                } catch (IOException e) {
                        Log.w("FileUtils", "Error writing " + file, e);
                } catch (Exception e) {
                        Log.w("FileUtils", "Failed to save file", e);
                } finally {
                        try {
                                if (null != os)
                                        os.close();
                        } catch (Exception ex) {

                        }
                }


                return success;
        }







        public void getWholeTableToNewSQLite(final String[] fieldNames, final Context context) {



                isGetWholeTableToArrayList =1;
                countYet=0;


                sqLiteDatabase = context.openOrCreateDatabase(tableName,MODE_PRIVATE,null);

                String fieldV = "";

                fieldV ="" + fieldNames[0]+ " Varchar";

                for (int counter=1;counter<fieldNames.length;counter++){

                        fieldV = fieldV + ", " + fieldNames[counter] + " Varchar";

                }

                sqLiteDatabase.execSQL("DROP TABLE "+ tableName);

                sqLiteDatabase.execSQL("CREATE TABLE IF NOT EXISTS " + tableName + " (" +
                        fieldV
                        + ")");


                firebaseRef = FirebaseDatabase.getInstance().getReference(tableName);



                AllfieldsArray = new ArrayList<>();


                firebaseRef.addValueEventListener(new ValueEventListener() {

                        String allFieldString;



                        @Override
                        public void onDataChange(DataSnapshot dataSnapshot) {

                                if(isGetWholeTableToArrayList==1) {

                                        dataCount = dataSnapshot.getChildrenCount();

                                        String[] fieldStrings = new String[45];


                                        for (DataSnapshot ds : dataSnapshot.getChildren()) {
                                                MyFirebaseManager.EntryClass mEntryClass = ds.getValue(MyFirebaseManager.EntryClass.class);
                                                //Start Here


                                                fieldStrings[0] = mEntryClass.getField1Data();
                                                fieldStrings[1] = mEntryClass.getField2Data();
                                                fieldStrings[2] = mEntryClass.getField3Data();
                                                fieldStrings[3] = mEntryClass.getField4Data();
                                                fieldStrings[4] = mEntryClass.getField5Data();
                                                fieldStrings[5] = mEntryClass.getField6Data();
                                                fieldStrings[6] = mEntryClass.getField7Data();
                                                fieldStrings[7] = mEntryClass.getField8Data();
                                                fieldStrings[8] = mEntryClass.getField9Data();
                                                fieldStrings[9] = mEntryClass.getField10Data();
                                                fieldStrings[10] = mEntryClass.getField11Data();
                                                fieldStrings[11] = mEntryClass.getField12Data();
                                                fieldStrings[12] = mEntryClass.getField13Data();
                                                fieldStrings[13] = mEntryClass.getField14Data();
                                                fieldStrings[14] = mEntryClass.getField15Data();
                                                fieldStrings[15] = mEntryClass.getField16Data();
                                                fieldStrings[16] = mEntryClass.getField17Data();
                                                fieldStrings[17] = mEntryClass.getField18Data();
                                                fieldStrings[18] = mEntryClass.getField19Data();
                                                fieldStrings[19] = mEntryClass.getField20Data();
                                                fieldStrings[20] = mEntryClass.getField21Data();
                                                fieldStrings[21] = mEntryClass.getField22Data();
                                                fieldStrings[22] = mEntryClass.getField23Data();
                                                fieldStrings[23] = mEntryClass.getField24Data();
                                                fieldStrings[24] = mEntryClass.getField25Data();
