                                                fieldStrings[31] = mEntryClass.getField32Data();
                                                fieldStrings[32] = mEntryClass.getField33Data();
                                                fieldStrings[33] = mEntryClass.getField34Data();
                                                fieldStrings[34] = mEntryClass.getField35Data();
